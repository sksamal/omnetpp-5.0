<config>
  <interface hosts='host1' names='eth0' address='10.0.1.2' netmask='255.255.255.0' mtu ='1500' metric='1' />
  <interface hosts='host2' names='eth0' address='10.0.2.2' netmask='255.255.255.0' mtu ='1500' metric='1' />
  <interface hosts='host3' names='eth0' address='10.0.3.2' netmask='255.255.255.0' mtu ='1500' metric='1' />
  <interface hosts='host4' names='eth0' address='10.0.4.2' netmask='255.255.255.0' mtu ='1500' metric='1' />
  <interface hosts='host5' names='eth0' address='10.0.5.2' netmask='255.255.255.0' mtu ='1500' metric='1' />

  <interface hosts='edge1' names='eth0' address='10.0.1.2' netmask='255.255.255.0' mtu ='1500' metric='1' />
  <interface hosts='edge1' names='obs' address='10.0.0.1' netmask='255.255.255.0' mtu ='1500' metric='1' />
  <interface hosts='edge2' names='eth0' address='10.0.2.2' netmask='255.255.255.0' mtu ='1500' metric='1' />
  <interface hosts='edge2' names='obs' address='10.0.0.2' netmask='255.255.255.0' mtu ='1500' metric='1' />

  <autoroute hosts='host1' destination='*' netmask='*' interface='eth0'/>
 <autoroute hosts='edge*' destination='*' netmask='*' interface='obs'/>
  
</config>
